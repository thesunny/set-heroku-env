{
  "name": "set-heroku-env",
  "version": "0.1.3",
  "description": "A CLI to read a dotenv file and use it to set the heroku env",
  "main": "index.js",
  "author": "Sunny Hirai",
  "license": "MIT",
  "files": [
    "bin/*"
  ],
  "bin": {
    "set-heroku-env": "./bin/set-heroku-env.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@types/debug": "^4.1.5",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.22",
    "babel-plugin-root-import": "^6.6.0",
    "rollup": "^2.38.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-sizes": "^1.0.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "minimist": "^1.2.5"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "rollup --config",
    "# prepare": "# automatically called during 'yarn publish'",
    "prepare": "yarn build",
    "dev:set": "babel-node ./scripts/set-heroku-env.ts --extensions='.ts' --dotenv=.env.heroku",
    "dev:unset": "babel-node ./scripts/set-heroku-env.ts --extensions='.ts' --dotenv=.env.unset",
    "dev:empty": "babel-node ./scripts/set-heroku-env.ts --extensions='.ts' --dotenv=.env.empty",
    "dev:missing": "babel-node ./scripts/set-heroku-env.ts --extensions='.ts'",
    "dev:not-found": "babel-node ./scripts/set-heroku-env.ts --extensions='.ts' --dotenv=.env.not-found",
    "update:kit": "git pull git@github.com:thesunny/cli-starter-kit.git"
  }
}
